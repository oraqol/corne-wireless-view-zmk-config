    macros {
        colemak: colemak {
            label = "colemak";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press &kp LGUI>, <&macro_press &kp SPACE>,  <&macro_release &kp SPACE>, <&macro_release &kp LGUI>;
        };
